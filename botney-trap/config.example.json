{
  "name": "botney-trap",
  "version": "1.0.0",
  "domain": "local.me",
  "clouds": {
    "aws": {
      "region": "us-west-2",
      "nodes": 3,
      "node_size": "t3.medium",
      "enabled": true
    },
    "digitalocean": {
      "region": "nyc1",
      "nodes": 3,
      "node_size": "s-2vcpu-2gb",
      "enabled": true
    }
  },
  "environments": [
    {
      "name": "production",
      "subdomain": "prod",
      "origins": ["aws", "digitalocean"]
    },
    {
      "name": "testing",
      "subdomain": "test",
      "origins": ["aws", "digitalocean"]
    },
    {
      "name": "development",
      "subdomain": "dev",
      "origins": ["digitalocean"]
    }
  ]
}
