{
    "name": "RobotMaker",
    "version": "1.0.0",
    "description": "",
    "scripts": {
        "start": "ts-node-dev -r dotenv/config -r tsconfig-paths/register ./src/index.ts | pino-pretty",
        "serve": "ts-node --transpile-only -r dotenv/config -r tsconfig-paths/register ./src/index.ts",
        "build": "tsc",
        "test": "jest",
        "test:nocache": "jest --no-cache --runInBand",
        "test:watch": "jest --watch --verbose false",
        "test:ci": "jest --forceExit --coverage --verbose --runInBand --no-cache",
        "lint": "eslint './src/**/*'",
        "pretty": "prettier --write './{src}/**/*.{ts,js,json,md}' && prettier --write './*.md'",
        "preinstall": "bash scripts/preinstall.sh"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{ts,js,json,md,scss}": [
            "prettier --write",
            "git add"
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/a-jaouen/gom-jabbar.git"
    },
    "keywords": [],
    "author": "Antoine Jaouen <antoine.jaouen@gmail.com>",
    "license": "UNLICENSED",
    "private": true,
    "engines": {
        "node": "14.15.1",
        "npm": "6.14.9"
    },
    "dependencies": {
        "@hapi/boom": "^9.1.0",
        "body-parser": "^1.19.0",
        "colors": "^1.4.0",
        "debug": "^4.3.1",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-pino-logger": "^5.0.0",
        "express-query-params": "^1.2.0",
        "express-unless": "^0.5.0",
        "pino": "^6.7.0",
        "pino-debug": "^1.3.0",
        "source-map-support": "^0.5.19",
        "swagger-ui-express": "^4.1.5",
        "ts-node": "^9.1.0",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^4.1.2",
        "yamljs": "^0.3.0"
    },
    "devDependencies": {
        "@types/boom": "^7.3.0",
        "@types/debug": "^4.1.5",
        "@types/express": "^4.17.9",
        "@types/express-unless": "^0.5.1",
        "@types/jest": "^26.0.16",
        "@types/node": "^14.14.10",
        "@types/pino": "^6.3.4",
        "@types/swagger-ui-express": "^4.1.2",
        "@typescript-eslint/eslint-plugin": "^4.9.0",
        "@typescript-eslint/parser": "^4.9.0",
        "eslint": "^7.15.0",
        "eslint-config-prettier": "^7.0.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-prettier": "^3.2.0",
        "husky": "^4.3.4",
        "jest": "^26.6.3",
        "lint-staged": "^10.5.3",
        "pino-pretty": "^4.3.0",
        "prettier": "^2.2.1",
        "ts-jest": "^26.4.4",
        "ts-node-dev": "^1.0.0"
    }
}
